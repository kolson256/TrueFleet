<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false"
    android:animateLayoutChanges="true"
    tools:context="app.truefleet.com.truefleet.Activitieis.OrderActivity">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/main_panel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        class="app.truefleet.com.truefleet.Fragments.MainPanelFragment" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/side_panel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        class="app.truefleet.com.truefleet.Fragments.SidePanelFragment" />

</RelativeLayout>
